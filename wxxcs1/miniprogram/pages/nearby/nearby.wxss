.outer,map{
  width:100%;
  height:100vh
}
.outer{
  position:relative;
}
.location{
position: absolute;
bottom:0;
z-index:100
}
.nav{
position: absolute;
top:0;
z-index:100;
background:  #f4f4f4;
border-bottom:1px solid #dfdfdf;
}
.nav text{
display:inline-block;
color: #90a772;

height:100rpx;
line-height: 100rpx;
border-right:1px solid #dfdfdf;
padding:22rpx;
}
.nav image{
  width:32rpx;
  height:32rpx;
}
scroll-view{
  height:600rpx;
  background:#fff;
}
scroll-view .content{
  border-bottom:1px solid #dfdfdf;
  width:100%;
  padding:30rpx;
}
scroll-view .title{
  font-size:16px;
}
scroll-view .address{
  font-size:10px;
  color:rgb(83, 78, 78)
}